{
    "name": "darcknico/comedor-web",
    "description": "Parte Web de la aplicacion del comedor",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "https://github.com/darcknico/comedor-rest",
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.2",
        "guzzlehttp/guzzle": "^6.2",
        "slim/flash": "^0.2.0",
        "slim/csrf": "^0.8.1",
        "teraone/twig-strftime-extension": "^1.0",
        "sofa/eloquence": "~5.1",
        "adbario/slim-secure-session-middleware": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
    	"psr-4": {
        "App\\": "src"
    	}
    },
	"scripts": {
        "start": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
	}

}
